{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryMsg",
  "description": "Query messages.",
  "oneOf": [
    {
      "description": "Query price for a denom. Implements the Stone OracleQueryMsg interface.",
      "type": "object",
      "required": [
        "price"
      ],
      "properties": {
        "price": {
          "type": "object",
          "required": [
            "denom"
          ],
          "properties": {
            "denom": {
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Query contract configuration.",
      "type": "object",
      "required": [
        "config"
      ],
      "properties": {
        "config": {
          "type": "object",
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Query price feed info for a denom.",
      "type": "object",
      "required": [
        "price_feed"
      ],
      "properties": {
        "price_feed": {
          "type": "object",
          "required": [
            "denom"
          ],
          "properties": {
            "denom": {
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Query all configured price feeds with pagination.",
      "type": "object",
      "required": [
        "all_price_feeds"
      ],
      "properties": {
        "all_price_feeds": {
          "type": "object",
          "properties": {
            "limit": {
              "type": [
                "integer",
                "null"
              ],
              "format": "uint32",
              "minimum": 0.0
            },
            "start_after": {
              "type": [
                "string",
                "null"
              ]
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    }
  ]
}
